{
    "manifest_version": 3,
    "name": "To-Do List",
    "version": "1.0",
    "description": "A simple to-do list Chrome extension.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "storage"
    ]
  }
  